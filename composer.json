{
    "name": "xtreamwayz/expressive-composer-installer",
    "type": "project",
    "homepage": "https://github.com/xtreamwayz/expressive-composer-installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Geert Eltink",
            "homepage": "https://xtreamwayz.github.io/"
        }
    ],
    "repositories": [{
        "type": "package",
        "package": {
            "name": "zendframework/zend-expressive",
            "version": "0.2.5",
            "dist": {
                "type": "vcs",
                "url": "https://github.com/weierophinney/zend-expressive/tree/feature/template-factories"
            }
        }
    }],
    "require": {
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "^0.2",
        "zendframework/zend-config": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/"
        }
    },
    "scripts": {
        "pre-install-cmd": "App\\Composer\\OptionalPackages::install",
        "pre-update-cmd": "App\\Composer\\OptionalPackages::install",
        "test": [
            "php vendor/squizlabs/php_codesniffer/scripts/phpcs",
            "php vendor/phpunit/phpunit/phpunit"
        ]
    }
}
