{
    "name": "xtreamwayz/expressive-composer-installer",
    "type": "project",
    "homepage": "https://github.com/xtreamwayz/expressive-composer-installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Geert Eltink",
            "homepage": "https://xtreamwayz.github.io/"
        }
    ],
    "require": {
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "^0.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Zend\\": "src/Zend/"
        }
    },
    "scripts": {
        "pre-install-cmd": "Zend\\Expressive\\Composer\\OptionalPackages::install",
        "pre-update-cmd": "Zend\\Expressive\\Composer\\OptionalPackages::install"
    }
}
